	</bean>

	<!-- 收费项目 -->
	<bean name="trainingProgramsAction" class="com.hode.train.action.TrainingProgramsAction"
		singleton="false">
		<property name="ibatisService">
			<ref bean="trainingProgramsServiceProxy" />
		</property>
	</bean>

	<bean name="courseAction" class="com.hode.train.action.CourseAction"
