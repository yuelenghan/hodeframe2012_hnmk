<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean name="trainApplyAction" class="com.hode.train.action.TrainApplyAction"
		singleton="false">
		<property name="ibatisService">
			<ref bean="trainApplyIbatisServiceProxy" />
		</property>
	</bean>
	<bean name="trainStudentAction" class="com.hode.train.action.TrainStudentAction"
